// Place your key bindings in this file to override the defaults
[
	{
		"key": "cmd+i",
		"command": "composerMode.agent"
	},
	{
		"key": "cmd+enter",
		"command": "workbench.action.debug.run",
		"when": "editorLangId == java"
	},
	{
		"key": "cmd+enter",
		"command": "python.execInTerminal",
		"when": "editorLangId == python"
	},
	{
		"key": "cmd+enter",
		"command": "notebook.cell.execute",
		"when": "notebookCellListFocused || notebookEditorFocused"
	},
	{
		"key": "shift+enter",
		"command": "notebook.cell.executeAndSelectBelow",
		"when": "notebookCellListFocused || notebookEditorFocused"
	},
	{
		"key": "shift+cmd+enter",
		"command": "notebook.execute",
		"when": "config.notebook.globalToolbar && !notebookHasSomethingRunning || config.notebook.globalToolbar && notebookInterruptibleKernel && !notebookHasSomethingRunning || config.notebook.globalToolbar && !notebookHasSomethingRunning && !notebookInterruptibleKernel"
	},
	{
		"key": "cmd+[Semicolon]",
		"command": "workbench.action.terminal.toggleTerminal",
		"when": "terminal.active"
	},
	{
		"key": "ctrl+shift+[Equal]",
		"command": "-workbench.action.terminal.toggleTerminal",
		"when": "terminal.active"
	},
	{
		"key": "ctrl+alt+cmd+enter",
		"command": "editor.action.inlineDiffs.acceptAll",
		"when": "editorTextFocus && @inlineDiffs.acceptAllEdits.isActiveEditorWithDiffs"
	},
	{
		"key": "cmd+enter",
		"command": "-editor.action.inlineDiffs.acceptAll",
		"when": "editorTextFocus && @inlineDiffs.acceptAllEdits.isActiveEditorWithDiffs"
	},
	{
		"key": "ctrl+alt+cmd+enter",
		"command": "refactorPreview.apply",
		"when": "refactorPreview.enabled && refactorPreview.hasCheckedChanges && focusedView == 'refactorPreview'"
	},
	{
		"key": "cmd+enter",
		"command": "-refactorPreview.apply",
		"when": "refactorPreview.enabled && refactorPreview.hasCheckedChanges && focusedView == 'refactorPreview'"
	},
	{
		"key": "alt+tab",
		"command": "editor.action.inlineSuggest.acceptNextWord",
		"when": "cppSuggestion && !editorReadonly || inlineSuggestionVisible && !editorReadonly"
	},
	{
		"key": "cmd+right",
		"command": "-editor.action.inlineSuggest.acceptNextWord",
		"when": "cppSuggestion && !editorReadonly || inlineSuggestionVisible && !editorReadonly"
	}
]